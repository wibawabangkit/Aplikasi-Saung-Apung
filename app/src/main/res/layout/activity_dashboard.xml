<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:layout_gravity="center"
        android:background="@drawable/pantai"
        android:layout_width="match_parent"
        android:layout_height="290dp">
        <ImageView
            android:id="@+id/imghari"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:scaleType="fitXY"/>
        <TextView
            android:layout_marginTop="50dp"
            android:textSize="15dp"
            fontPath="fonts/Poppins-SemiBold.otf"
            tools:ignore="MissingPrefix"
            android:gravity="center"
            android:textColor="@color/black"
            android:id="@+id/ucap"
            android:text="Selamat Dan Sukses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/kamu"
            fontPath="fonts/Poppins-Black.otf"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/ucap"
            android:gravity="center"
            android:text="Apa Kabar Anda Hari Ini ?"
            android:textColor="@color/black"
            android:textSize="15dp"
            tools:ignore="MissingPrefix" />
        <RelativeLayout
            android:layout_alignParentBottom="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:id="@+id/car4"
                card_view:cardCornerRadius="10dp"
                android:layout_margin="11dp"
                card_view:cardElevation="5dp">
                <RelativeLayout
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        fontPath="fonts/Poppins-SemiBold.otf"
                        tools:ignore="MissingPrefix"
                        android:textSize="17sp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/black"
                        android:layout_marginLeft="15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Saldo Gopay Anda : "/>

                    <TextView
                        android:layout_toLeftOf="@id/home_mPayBalance"
                        fontPath="fonts/Poppins-Medium.otf"
                        tools:ignore="MissingPrefix"
                        android:textSize="10dp"
                        android:textColor="@color/black"
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Rp. "/>
                    <TextView
                        fontPath="fonts/Poppins-SemiBold.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/black"
                        android:id="@+id/home_mPayBalance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="15dp"
                        android:text="@string/text_updating"
                        android:textSize="17sp" />
                    <LinearLayout
                        android:id="@+id/garis"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        android:layout_below="@id/home_mPayBalance"
                        android:layout_width="match_parent"
                        android:background="@color/black"
                        android:layout_height="1dp"/><LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_below="@id/garis"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:background="?attr/selectableItemBackground"
                        android:id="@+id/home_topUpButton"
                        android:layout_weight="0.50"
                        android:layout_marginLeft="25dp"
                        android:layout_width="match_parent"
                        android:layout_height="140dp">
                        <ImageView
                            android:layout_centerHorizontal="true"
                            android:id="@+id/imdompet"
                            android:layout_width="85dp"
                            android:layout_height="85dp"
                            android:src="@drawable/ic_dompet"/>
                        <TextView
                            fontPath="fonts/Poppins-Medium.otf"
                            tools:ignore="MissingPrefix"
                            android:layout_below="@id/imdompet"
                            android:textColor="@color/black"
                            android:gravity="center"
                            android:textSize="14dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Isi Ulang"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:background="?attr/selectableItemBackground"
                        android:layout_weight="0.50"
                        android:layout_marginRight="25dp"
                        android:layout_width="match_parent"
                        android:layout_height="150dp">
                        <ImageView
                            android:layout_centerHorizontal="true"
                            android:id="@+id/impoin"
                            android:layout_width="85dp"
                            android:layout_height="85dp"
                            android:src="@drawable/ic_poin"/>
                        <TextView
                            fontPath="fonts/Poppins-Medium.otf"
                            tools:ignore="MissingPrefix"
                            android:textColor="@color/black"
                            android:layout_below="@id/impoin"
                            android:gravity="center"
                            android:textSize="14dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Point Anda"/>
                    </RelativeLayout>
                </LinearLayout>
                </RelativeLayout>
                <ImageView
                    android:visibility="gone"
                    android:layout_width="40dp"
                    android:layout_marginRight="10dp"
                    android:layout_height="40dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_icon_wallet"
                    android:textColor="@color/white" />
            </androidx.cardview.widget.CardView>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:gravity="center_vertical"
        android:layout_gravity="center"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/txte"
            fontPath="fonts/Poppins-Bold.otf"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center"
            android:text="Siap melayani anda setulus hati"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:ignore="MissingPrefix" />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="1">
        <androidx.cardview.widget.CardView
            android:layout_marginLeft="5dp"
            card_view:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/home_mRide"
            android:layout_width="0dp"
            card_view:cardElevation="5dp"
            android:layout_height="100dp"
            android:layout_weight="0.33"
            android:clickable="true">
            <RelativeLayout
                android:background="?attr/selectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_marginTop="10dp"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:layout_above="@+id/textView3"
                    android:layout_centerHorizontal="true"
                    android:scaleType="center"
                    android:src="@drawable/ic_e_ride_on" />
                <TextView
                    fontPath="fonts/Poppins-SemiBold.otf"
                    tools:ignore="MissingPrefix"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:text="@string/home_mRide"
                    android:textColor="@color/greyText"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold"
                    android:id="@+id/textView3"
                    android:layout_marginBottom="5dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_marginLeft="5dp"
            card_view:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/home_mSend"
            android:layout_width="0dp"
            card_view:cardElevation="5dp"
            android:layout_height="100dp"
            android:layout_weight="0.33"
            android:clickable="true">
            <RelativeLayout
                android:background="?attr/selectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_marginTop="10dp"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_above="@+id/textView2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:scaleType="center"
                    android:src="@drawable/ic_ebox" />
                <TextView
                    fontPath="fonts/Poppins-SemiBold.otf"
                    tools:ignore="MissingPrefix"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:text="@string/home_mSend"
                    android:textColor="@color/greyText"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold"
                    android:id="@+id/textView2"
                    android:layout_marginBottom="5dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_marginLeft="5dp"
            card_view:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/home_mCar"
            android:layout_width="0dp"
            card_view:cardElevation="5dp"
            android:layout_height="100dp"
            android:layout_weight="0.33"
            android:clickable="true">
            <RelativeLayout
                android:background="?attr/selectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView6"
                    android:layout_centerHorizontal="true"
                    android:scaleType="center"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/ic_ecar" />
                <TextView
                    fontPath="fonts/Poppins-SemiBold.otf"
                    tools:ignore="MissingPrefix"
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:maxLines="1"
                    android:text="@string/home_mCar"
                    android:textColor="@color/greyText"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="1">
        <androidx.cardview.widget.CardView
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            card_view:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/home_mFood"
            android:layout_width="0dp"
            card_view:cardElevation="5dp"
            android:layout_height="100dp"
            android:layout_weight="0.33"
            android:clickable="true">
            <RelativeLayout
                android:background="?attr/selectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/GoFood"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="center"
                    android:src="@drawable/ic_efood"
                    android:layout_above="@+id/textView12"
                    android:layout_marginBottom="5dp"
                    android:layout_centerHorizontal="true"/>
                <TextView
                    fontPath="fonts/Poppins-SemiBold.otf"
                    tools:ignore="MissingPrefix"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:text="@string/home_mFood"
                    android:textColor="@color/greyText"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold"
                    android:id="@+id/textView12"
                    android:layout_marginBottom="5dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            card_view:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/home_mLaundry"
            android:layout_width="0dp"
            card_view:cardElevation="5dp"
            android:background="?attr/selectableItemBackground"
            android:layout_height="100dp"
            android:layout_weight="0.33"
            android:clickable="true">
            <RelativeLayout
                android:background="?attr/selectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="center"
                    android:src="@drawable/ic_elaundry"
                    android:layout_above="@+id/textView18"
                    android:layout_marginBottom="5dp"
                    android:layout_centerHorizontal="true" />
                <TextView
                    fontPath="fonts/Poppins-SemiBold.otf"
                    tools:ignore="MissingPrefix"
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:maxLines="1"
                    android:text="@string/home_mLaundry"
                    android:textColor="@color/greyText"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold" /></RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            card_view:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/home_mTix"
            android:layout_width="0dp"
            card_view:cardElevation="5dp"
            android:background="?attr/selectableItemBackground"
            android:layout_height="100dp"
            android:layout_weight="0.33"
            android:clickable="true">
            <RelativeLayout
                android:background="?attr/selectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="center"
                    android:src="@drawable/ic_etix"
                    android:layout_above="@+id/textView21"
                    android:layout_marginBottom="5dp"
                    android:layout_centerHorizontal="true" />
                <TextView
                    fontPath="fonts/Poppins-SemiBold.otf"
                    tools:ignore="MissingPrefix"
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:maxLines="1"
                    android:text="@string/home_mTix"
                    android:textColor="@color/greyText"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

    </LinearLayout>
    <Button
        android:id="@+id/out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:layout_marginBottom="30dp"
        android:background="@color/biru"
        android:text="SIGN OUT"
        android:textColor="#F8F3F3F3" />
</LinearLayout>